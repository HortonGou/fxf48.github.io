  "scripts": {
    "7DF6C94E-302C-4AC0-827D-DF97E3F428ED": [
      {
        "name": "",
        "source": "var sprinkle = scene.getObjectByName( 'Sprinkle' );\n\nvar group = new THREE.Group();\nscene.add( group );\n\nvar materials = [\n\tnew THREE.MeshStandardMaterial( { color: 0xbb00ff, roughness: 0.1, metalness: 0 } ),\n\tnew THREE.MeshStandardMaterial( { color: 0xffff00, roughness: 0.1, metalness: 0 } ),\n\tnew THREE.MeshStandardMaterial( { color: 0xff00bb, roughness: 0.1, metalness: 0 } ),\n\tnew THREE.MeshStandardMaterial( { color: 0xffffff, roughness: 0.1, metalness: 0 } )\n];\n\nfor ( var i = 0; i < 1000; i ++ ) {\n\n\tvar clone = sprinkle.clone();\n\tclone.material = materials[ Math.floor( Math.random() * materials.length ) ];\n\tclone.position.x = Math.random() - 0.5;\n\tclone.position.y = Math.random() - 0.5;\n\tclone.position.z = Math.random() - 0.5;\n\tclone.position.normalize().multiplyScalar( 4.1 );\n\tclone.lookAt( 0, 0, 0 );\n\t// clone.rotation.x = Math.random() * Math.PI * 2;\n\tclone.rotation.y = Math.random() * Math.PI * 2;\n\tclone.rotation.z = Math.random() * Math.PI * 2;\n\tclone.scale.y = Math.random() + 0.5;\n\tgroup.add( clone );\n\t\n}\n\nfunction update( event ) {\n\n\tvar time = Date.now();\n\t\n\tscene.rotation.y = time / 10000;\n\tscene.rotation.z = time / 20000;\n\n}"
